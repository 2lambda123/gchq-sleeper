{
  "executionRoleArn": "cluster-role",
  "jobDriver": {
    "sparkSubmit": {
      "entryPoint": "/workdir/bulk-import-runner.jar",
      "entryPointArguments": [
        "config-bucket",
        "my-job",
        "sleeper-instance-table-name-my-job",
        "run-id"
      ],
      "sparkSubmitParameters": "--class sleeper.bulkimport.job.runner.dataframelocalsort.BulkImportDataframeLocalSortDriver --conf spark.executorEnv.JAVA_HOME=/usr/lib/jvm/jre-11 --conf spark.emr-serverless.driverEnv.JAVA_HOME=/usr/lib/jvm/jre-11 --conf spark.executor.cores=4 --conf spark.executor.memory=16g --conf spark.executor.instances=28 --conf spark.driver.cores=4 --conf spark.driver.memory=16g"
    }
  },
  "configurationOverrides": {
    "monitoringConfiguration": {
      "s3MonitoringConfiguration": {
        "logUri": "s3://import-bucket"
      }
    }
  },
  "name": "job-run-id"
}